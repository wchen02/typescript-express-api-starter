{
  "name": "rw-api",
  "version": "1.0.1",
  "description": "RW API",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && NODE_CONFIG_DIR='./src/config' node build/index.js",
    "dev": "NODE_CONFIG_DIR='./src/config' DEBUG=* nodemon",
    "build": "npm run lint && rimraf ./build && tsc",
    "commit": "git-cz",
    "release": "npm run build && standard-version",
    "release:tags": "git push --follow-tags origin master",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix"
  },
  "dependencies": {
    "@types/config": "0.0.36",
    "@types/dotenv": "^8.2.0",
    "@types/knex": "^0.16.1",
    "@types/pg": "^7.14.1",
    "config": "^3.2.5",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "knex": "^0.20.8",
    "morgan": "^1.9.1",
    "pg": "^7.17.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-plugin-import": "^2.20.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "keywords": [
    "TypeScript",
    "Node.js",
    "Express",
    "API",
    "Starter Template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wchen02/rw-api.git"
  },
  "author": "Wensheng Chen <hi@wenshengchen.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wchen02/rw-api/issues"
  },
  "homepage": "https://github.com/wchen02/rw-api#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
